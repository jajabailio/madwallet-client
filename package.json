{
  "name": "mad-wallet-client",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:check": "tsc && vite build",
    "preview": "vite preview",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.4",
    "@mui/x-date-pickers": "^8.22.1",
    "axios": "^1.12.2",
    "date-fns": "^4.1.0",
    "joi": "^18.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.11.0",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "tsc-files": "^1.1.4",
    "typescript": "^5.9.3",
    "vite": "^7.1.11",
    "vitest": "^4.0.16"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "biome check --write --no-errors-on-unmatched",
      "tsc-files --noEmit"
    ]
  }
}
